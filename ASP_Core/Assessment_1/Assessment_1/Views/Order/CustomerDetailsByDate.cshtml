@model List<Assessment_1.Models.Customer>

<h2>Customers who Placed Orders on @ViewBag.OrderDate</h2>

<table class="table">
    <thead>
        <tr>
            <th>Customer ID</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.CustomerId</td>
                <td>@customer.CustomerTypes</td>
            </tr>
        }
    </tbody>
</table>
